@model Mentor_Management_System.Models.FacultyModel

@{
    ViewData["Title"] = "FacultyCreate";
    Layout = "~/Views/Shared/_AdminDashboardLayout.cshtml";
}


<div class="row">
    <div class="col-md-12">
        <div class="h3 text-center bg-primary py-2 my-2">Create Faculty</div>
        <form asp-action="FacultyCreate" method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group mb-3">
                <label asp-for="FacultyName" class="control-label"></label>
                <input asp-for="FacultyName" class="form-control" />
                <span asp-validation-for="FacultyName" class="text-danger"></span>
            </div>
            <div class="form-group mb-3">
                <label asp-for="FacultyEmail" class="control-label"></label>
                <input asp-for="FacultyEmail" class="form-control" />
                <span asp-validation-for="FacultyEmail" class="text-danger"></span>
            </div>
            <div class="form-group mb-3">
                <label asp-for="FacultyQualification" class="control-label"></label>
                <input asp-for="FacultyQualification" class="form-control" />
                <span asp-validation-for="FacultyQualification" class="text-danger"></span>
            </div>
            <div class="form-group mb-3">
                <label asp-for="FacultyCourse" class="control-label"></label>
                <input asp-for="FacultyCourse" class="form-control" />
                <span asp-validation-for="FacultyCourse" class="text-danger"></span>
            </div>
            <div class="form-group mb-3">
                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group mb-3">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



